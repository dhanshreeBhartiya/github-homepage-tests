{
    "compilerOptions": {
      "target": "es6",
      "lib": ["es6", "dom"],
      "module": "commonjs",
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": false,
      "types": ["cypress"],
      "baseUrl": "./",
      "paths": {
        "@integration/*": ["cypress/integration/*"],
        "@fixtures/*": ["cypress/fixtures/*"]
    }   
},
"include": ["cypress/**/*.ts"]
}
  